{
    "manifest_version": 2,
    "name": "永远点不到的搜索按钮",
    "version": "1.0",
    "description": "让你永远也点击不到 Baidu 的搜索按钮",
    "content_scripts": [
        {
            "matches": ["*://www.baidu.com/", "*://www.google.com/"],
            "js": ["js/cannot_touch_baidu.js", "js/cannot_touch_google.js"]
        }
    ]
}